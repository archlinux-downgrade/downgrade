#: bin/downgrade:7
msgid "Usage: downgrade [option...] <pkg> [pkg...] [-- pacman_option...]"
msgstr "用法：downgrade [option...] <包> [包...] [-- pacman_option...]"

#: bin/downgrade:9
msgid "Options"
msgstr "选件"

#: bin/downgrade:10
msgid "command"
msgstr "命令"

#: bin/downgrade:11
msgid "pacman command to use, defaults to"
msgstr "使用的 pacman 命令，默认为"

#: bin/downgrade:12 bin/downgrade:14 bin/downgrade:18
msgid "path"
msgstr "文件路径"

#: bin/downgrade:13
msgid "pacman configuration file, defaults to"
msgstr "pacman 配置文件，默认为"

#: bin/downgrade:15
msgid "pacman cache directory,"
msgstr "pacman 缓存目录，"

#: bin/downgrade:16
msgid "default value(s) taken from pacman configuration file,"
msgstr "取自 pacman 配置文件的默认值，"

#: bin/downgrade:17 bin/downgrade:21
msgid "or otherwise defaults to"
msgstr "否则默认为"

#: bin/downgrade:19
msgid "pacman log file,"
msgstr "pacman 日志文件"

#: bin/downgrade:20
msgid "default value taken from pacman configuration file,"
msgstr "取自 pacman 配置文件的默认值，"

#: bin/downgrade:22
msgid "integer"
msgstr "整数"

#: bin/downgrade:23
msgid "maximum depth to search for cached packages, defaults to"
msgstr "搜索缓存包的最大深度，默认为"

#: bin/downgrade:25
msgid "location of ALA server, defaults to"
msgstr "ALA 服务器的位置，默认为"

#: bin/downgrade:26
msgid "only use ALA server"
msgstr "仅使用 ALA 服务器"

#: bin/downgrade:27
msgid "only use cached packages"
msgstr "只使用缓存的包"

#: bin/downgrade:29
msgid "whether to add packages to IgnorePkg"
msgstr "是否添加软件包到 IgnorePkg"

#: bin/downgrade:30
msgid "show downgrade version"
msgstr "显示 downgrade 版本"

#: bin/downgrade:31
msgid "show help script"
msgstr "显示帮助脚本"

#: bin/downgrade:33
msgid "Note"
msgstr "注意"

#: bin/downgrade:34
msgid "Options after the -- characters will be treated as pacman options."
msgstr "-- 字符后的选项将被视为 pacman 选项。"

#: bin/downgrade:35
msgid "See downgrade(8) for details."
msgstr "详情请查看 downgrade(8)。"

#: bin/downgrade:97
msgid "Available packages"
msgstr "可选的包"

#: bin/downgrade:127
#, sh-format
msgid "add $pkg to IgnorePkg? [y/N] "
msgstr "添加 $pkg 到 IgnorePkg? [y/N] "

#: bin/downgrade:129
msgid "y"
msgstr "y"

#: bin/downgrade:239
msgid "remote"
msgstr "远端"

#: bin/downgrade:288
msgid "No results found"
msgstr "未找到结果"

#: bin/downgrade:299
msgid "Invalid choice"
msgstr "选择无效"

#: bin/downgrade:312
#, sh-format
msgid "Unable to downgrade $name"
msgstr "无法降级 $name"

#: bin/downgrade:341
msgid "Missing --pacman argument"
msgstr "缺少 --pacman 参数"

#: bin/downgrade:354
msgid "Missing --pacman-conf argument"
msgstr "缺少 --pacman-conf 参数"

#: bin/downgrade:367
msgid "Missing --ala-url argument"
msgstr "缺少 --ala-url 参数"

#: bin/downgrade:380
msgid "Missing --pacman-cache argument"
msgstr "缺少 --pacman-cache 参数"

#: bin/downgrade:393
msgid "Missing --pacman-log argument"
msgstr "缺少 --pacman-log 参数"

#: bin/downgrade:406
msgid "Missing --maxdepth argument"
msgstr "缺少 --maxdepth 参数"

#: bin/downgrade:427
msgid "Missing or wrong --ignore argument"
msgstr "--pacman 参数缺失或有误"

#: bin/downgrade:444
#, sh-format
msgid "Unrecognized option $current_option"
msgstr "无法识别的选项 $current_option"

#: bin/downgrade:459
msgid "No packages provided for downgrading"
msgstr "没有提供降级包"

#: bin/downgrade:526
msgid "downgrade must be run as root"
msgstr "downgrade 必须以 root 身份运行"

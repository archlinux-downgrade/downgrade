msgstr "s"

#: downgrade.in:199
msgid "local"
msgstr "local"

#: downgrade.in:201
msgid "remote"
msgstr "remoto"

#: downgrade.in:251
msgid "Invalid choice"
msgstr "Elección inválida"

#: downgrade.in:283
#, sh-format
msgid "Unable to downgrade $term"
msgstr "No se puede degradar $term"

#: downgrade.in:328

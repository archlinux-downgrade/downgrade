#~ msgstr "lokalnie"

#~ msgid "do not use sudo even when available, use su"
#~ msgstr "nie używaj sudo, nawet jeśli jest dostępne, używaj su"

#~ msgid "architecture"
#~ msgstr "architektura"

#~ msgid "target architecture, defaults to output of"
#~ msgstr "architektura docelowa, domyślnie wyjście"

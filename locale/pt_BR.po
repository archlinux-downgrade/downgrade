#, sh-format
msgid "add $pkg to IgnorePkg? [y/n] "
msgstr "adicionar $pkg em IgnorePkg? [s/n]"

#: downgrade:88
msgid "y"
msgstr "s"

#: downgrade:163
msgid "local"
msgstr "local"

#: downgrade:165
msgid "remote"
msgstr "remoto"

#: downgrade:215
msgid "Invalid choice"
msgstr "Escolha inválida"

#: downgrade:242
#, sh-format
msgid "Unable to downgrade $term"
msgstr "Não foi possível fazer o downgrade $term"

#: downgrade:286
